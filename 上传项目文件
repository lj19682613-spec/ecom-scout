        { value: 265, itemStyle: { color: new echarts.graphic.LinearGradient(0,0,1,0,[{offset:0,color:'#8b5cf633'},{offset:1,color:PALETTE.violet}]), borderRadius: [0,6,6,0] } },
        { value: 819, itemStyle: { color: new echarts.graphic.LinearGradient(0,0,1,0,[{offset:0,color:'#fbbf2433'},{offset:1,color:PALETTE.amber}]), borderRadius: [0,6,6,0] } },
