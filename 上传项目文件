  &lt;div class="max-w-6xl mx-auto"&gt;
    &lt;div class="reveal"&gt;
      &lt;div class="flex items-center gap-3 mb-2"&gt;
        &lt;span class="font-mono text-xs text-cyan-400 tracking-widest"&gt;07&lt;/span&gt;
        &lt;div class="glow-line flex-1"&gt;&lt;/div&gt;
      &lt;/div&gt;
      &lt;h2 class="text-3xl md:text-4xl font-bold mb-4"&gt;行动建议&lt;/h2&gt;
      &lt;p class="text-slate-400 mb-12"&gt;本周优先执行事项&lt;/p&gt;
    &lt;/div&gt;

    &lt;div class="space-y-4"&gt;
      &lt;div class="glass-card p-6 reveal" style="border-left: 3px solid #f43f5e;"&gt;
        &lt;div class="flex items-start gap-4"&gt;
          &lt;div class="font-mono text-3xl font-bold text-rose-500/30"&gt;P1&lt;/div&gt;
          &lt;div&gt;
