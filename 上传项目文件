// ============================================================
// Heat Map Grid
// ============================================================
(function () {
  const categories = [
    { name: '美妆个护', tiktok: 'hot', instagram: 'hot', amazon: 'hot', etsy: 'hot' },
    { name: '健康养生/补剂', tiktok: 'hot', instagram: 'hot', amazon: 'hot', etsy: 'cold' },
    { name: '家居小家电/智能家居', tiktok: 'hot', instagram: 'cold', amazon: 'hot', etsy: 'cold' },
    { name: '宠物用品', tiktok: 'rising', instagram: 'cold', amazon: 'rising', etsy: 'cold' },
    { name: '数字下载/模板', tiktok: 'cold', instagram: 'cold', amazon: 'cold', etsy: 'hot' },
    { name: '环保可持续', tiktok: 'cold', instagram: 'rising', amazon: 'cold', etsy: 'rising' },
    { name: '健身器材', tiktok: 'hot', instagram: 'cold', amazon: 'hot', etsy: 'cold' },
    { name: '收藏玩偶/潮玩', tiktok: 'explosive', instagram: 'cold', amazon: 'cold', etsy: 'cold' },
    { name: '厨房小工具', tiktok: 'rising', instagram: 'cold', amazon: 'rising', etsy: 'cold' },
    { name: '珠宝饰品', tiktok: 'cold', instagram: 'hot', amazon: 'cold', etsy: 'hot' }
  ];

  const labels = { hot: '🔥 HOT', rising: '📈 上升', explosive: '💥 爆发', cold: '—' };
  const classes = { hot: 'heat-hot', rising: 'heat-rising', explosive: 'heat-explosive', cold: 'heat-cold' };

  const container = document.getElementById('heat-rows');
  categories.forEach(cat =&gt; {
    const row = document.createElement('div');
    row.className = 'grid grid-cols-5 gap-2';
    row.innerHTML = `
      &lt;div class="flex items-center text-xs text-slate-300 font-medium py-2 px-3"&gt;${cat.name}&lt;/div&gt;
      &lt;div class="heat-cell ${classes[cat.tiktok]}"&gt;${labels[cat.tiktok]}&lt;/div&gt;
      &lt;div class="heat-cell ${classes[cat.instagram]}"&gt;${labels[cat.instagram]}&lt;/div&gt;
      &lt;div class="heat-cell ${classes[cat.amazon]}"&gt;${labels[cat.amazon]}&lt;/div&gt;
      &lt;div class="heat-cell ${classes[cat.etsy]}"&gt;${labels[cat.etsy]}&lt;/div&gt;
    `;
    container.appendChild(row);
  });
})();

