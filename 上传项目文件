      cost: '$5.88', price: '$24.99-$39.99', margin: '65-75%', marginMid: 70,
      comp: 'Great (低)', trend: 'Evergreen', color: PALETTE.emerald,
      platforms: ['TikTok', 'Amazon'],
      reason: '通用痛点，社交媒体演示效果极佳，常青品类'
    },
    {
      rank: 9, name: '3D动物陶瓷杯', nameEn: 'Ceramic Animal Mug',
      cost: '$5-8', price: '$16.99-$24.99', margin: '50-65%', marginMid: 57,
      comp: 'Moderate', trend: 'Growing', color: PALETTE.orange,
      platforms: ['Etsy', 'Amazon'],
      reason: 'Etsy 4.9星好评，强礼品属性'
    },
    {
      rank: 10, name: '智能门把手', nameEn: 'Smart Door Knob',
      cost: '$38.79', price: '$59.99-$69.99', margin: '35-45%', marginMid: 40,
      comp: 'Average', trend: '5年稳步上升', color: PALETTE.slate,
      platforms: ['Amazon', 'TikTok'],
      reason: '智能家居大趋势，安全需求刚性'
    }
  ];

  const container = document.getElementById('product-cards');

  products.forEach(p =&gt; {
    const card = document.createElement('div');
    card.className = 'product-card glass-card p-5 md:p-6 reveal relative';
    card.style.cssText = `--accent: ${p.color};`;
    card.innerHTML = `
      &lt;div style="position:absolute;top:0;left:0;width:4px;height:100%;border-radius:4px 0 0 4px;background:${p.color};"&gt;&lt;/div&gt;
      &lt;div class="rank-num" style="color:${p.color};"&gt;${String(p.rank).padStart(2,'0')}&lt;/div&gt;
      &lt;div class="flex flex-col md:flex-row md:items-start gap-4"&gt;
        &lt;div class="flex-1 min-w-0"&gt;
          &lt;div class="flex items-center gap-2 flex-wrap mb-1"&gt;
            &lt;h3 class="text-base font-bold"&gt;${p.name}&lt;/h3&gt;
            &lt;span class="font-mono text-xs text-slate-500"&gt;${p.nameEn}&lt;/span&gt;
          &lt;/div&gt;
          &lt;p class="text-xs text-slate-400 mb-3"&gt;${p.reason}&lt;/p&gt;
          &lt;div class="flex items-center gap-2 flex-wrap"&gt;
            ${p.platforms.map(pl =&gt; `&lt;span class="glass-card-sm px-2 py-0.5 text-[10px] text-slate-400"&gt;${pl}&lt;/span&gt;`).join('')}
            &lt;span class="badge ${p.trend.includes('Rising') || p.trend.includes('Growing') ? 'badge-rising' : p.trend === 'Evergreen' || p.trend.includes('Steady') ? 'badge-steady' : 'badge-hot'}"&gt;${p.trend}&lt;/span&gt;
          &lt;/div&gt;
        &lt;/div&gt;
        &lt;div class="flex gap-4 md:gap-6 text-right shrink-0"&gt;
          &lt;div&gt;
            &lt;div class="text-[10px] text-slate-500 mb-0.5"&gt;采购成本&lt;/div&gt;
            &lt;div class="font-mono text-sm text-slate-300"&gt;${p.cost}&lt;/div&gt;
          &lt;/div&gt;
          &lt;div&gt;
            &lt;div class="text-[10px] text-slate-500 mb-0.5"&gt;售价区间&lt;/div&gt;
            &lt;div class="font-mono text-sm text-cyan-400"&gt;${p.price}&lt;/div&gt;
          &lt;/div&gt;
          &lt;div&gt;
            &lt;div class="text-[10px] text-slate-500 mb-0.5"&gt;预估利润率&lt;/div&gt;
            &lt;div class="font-mono text-sm font-bold" style="color:${PALETTE.emerald};"&gt;${p.margin}&lt;/div&gt;
          &lt;/div&gt;
        &lt;/div&gt;
      &lt;/div&gt;
      &lt;div class="mt-3"&gt;
        &lt;div class="margin-bar-bg"&gt;
          &lt;div class="margin-bar-fill" style="width:0%;background: linear-gradient(90deg, ${p.color}66, ${p.color});" data-width="${p.marginMid}%"&gt;&lt;/div&gt;
        &lt;/div&gt;
      &lt;/div&gt;
    `;
    container.appendChild(card);
  });

  // Re-init reveals and bars for dynamically added elements
  container.querySelectorAll('.reveal').forEach(el =&gt; {
    gsap.fromTo(el,
      { opacity: 0, y: 40 },
      {
        opacity: 1, y: 0, duration: 0.8, ease: 'power3.out',
        scrollTrigger: { trigger: el, start: 'top 88%', toggleActions: 'play none none none' }
      }
    );
  });

  container.querySelectorAll('.margin-bar-fill[data-width]').forEach(bar =&gt; {
    ScrollTrigger.create({
      trigger: bar,
      start: 'top 92%',
      onEnter: () =&gt; { bar.style.width = bar.dataset.width; }
    });
  });
})();

// ============================================================
// Chart 5 — Blue Ocean Quadrant
// ============================================================
(function () {
  const chart = initChart('chart-quadrant');
  if (!chart) return;

  // [competition (x: 0-10), demand (y: 0-10), name, quadrant]
  const data = [
    // Blue Ocean (low comp, high demand)
    [2, 8.5, '宠物健康科技配件', 'blue'],
    [2.5, 7.5, '环保厨房小工具', 'blue'],
    [1.8, 7, '心理健康日记套装', 'blue'],
    [1.5, 8, '个性化数字模板', 'blue'],
    [2.2, 7.2, '硅胶收纳袋', 'blue'],
    // Red Ocean (high comp, high demand)
    [8, 9, '美妆/护肤', 'red'],
    [7.5, 7.5, '手机配件', 'red'],
    [7, 7, 'LED 灯带', 'red'],
    // Low both
    [2, 2.5, 'VR/AR 配件', 'gray'],
    [2.5, 2, '木制茶壶', 'gray'],
    // High comp low demand
    [8.5, 2.5, '大众时装', 'warn']
  ];

  const colorMap = {
    blue: PALETTE.cyan,
    red: PALETTE.rose,
    gray: PALETTE.slate,
    warn: PALETTE.amber
  };

  chart.setOption({
    tooltip: { ...tooltipStyle(),
      formatter: p =&gt; {
        const d = p.data;
        const q = { blue: '🌊 蓝海机会', red: '🔴 红海竞争', gray: '⚪ 观望区域', warn: '⚠️ 避开区域' };
        return `&lt;b&gt;${d[2]}&lt;/b&gt;&lt;br/&gt;${q[d[3]]}&lt;br/&gt;竞争度: ${d[0]}/10&lt;br/&gt;需求度: ${d[1]}/10`;
      }
    },
    grid: { left: 70, right: 40, top: 40, bottom: 60 },
    xAxis: {
      name: '竞争度 →',
      nameTextStyle: { color: '#64748b', fontSize: 12, fontFamily: "'Noto Sans SC',sans-serif", padding: [10,0,0,0] },
      min: 0, max: 10,
      axisLine: { lineStyle: { color: '#1e293b' } },
      axisTick: { show: false },
      axisLabel: { color: '#475569', fontSize: 10, fontFamily: 'Space Mono' },
      splitLine: { show: false }
    },
    yAxis: {
      name: '需求度 →',
      nameTextStyle: { color: '#64748b', fontSize: 12, fontFamily: "'Noto Sans SC',sans-serif" },
      min: 0, max: 10,
      axisLine: { lineStyle: { color: '#1e293b' } },
      axisTick: { show: false },
      axisLabel: { color: '#475569', fontSize: 10, fontFamily: 'Space Mono' },
      splitLine: { show: false }
    },
    // Quadrant backgrounds
    graphic: [
      // Blue ocean zone
      { type: 'rect', left: 70, top: 40, shape: { width: 240, height: 200 },
        style: { fill: 'rgba(56, 189, 248, 0.04)', stroke: 'rgba(56, 189, 248, 0.08)', lineWidth: 1, lineDash: [4, 4] } },
      { type: 'text', left: 80, top: 50,
        style: { text: '🌊 蓝海区域', fill: 'rgba(56, 189, 248, 0.4)', fontSize: 13, fontFamily: "'Noto Sans SC',sans-serif" } },
      // Red ocean zone
      { type: 'rect', right: 40, top: 40, shape: { width: 200, height: 200 },
        style: { fill: 'rgba(244, 63, 94, 0.04)', stroke: 'rgba(244, 63, 94, 0.08)', lineWidth: 1, lineDash: [4, 4] } },
      { type: 'text', right: 50, top: 50,
        style: { text: '🔴 红海区域', fill: 'rgba(244, 63, 94, 0.4)', fontSize: 13, fontFamily: "'Noto Sans SC',sans-serif" } },
      // Center divider lines
      { type: 'line', shape: { x1: 0, y1: 0, x2: 0, y2: 500 },
        left: '50%', top: 40,
        style: { stroke: '#1e293b', lineWidth: 1, lineDash: [6, 4] } },
      { type: 'line', shape: { x1: 0, y1: 0, x2: 600, y2: 0 },
        left: 70, top: '52%',
        style: { stroke: '#1e293b', lineWidth: 1, lineDash: [6, 4] } }
    ],
    series: [{
      type: 'scatter',
      symbolSize: 18,
      data: data.map(d =&gt; ({
        value: d,
        itemStyle: { color: colorMap[d[3]], shadowBlur: 12, shadowColor: colorMap[d[3]] + '44' }
      })),
      label: {
        show: true, position: 'right',
        formatter: p =&gt; p.data.value[2],
        color: '#94a3b8', fontSize: 11, fontFamily: "'Noto Sans SC',sans-serif",
        distance: 8
      },
      animationDuration: 2000
    }]
  });
})();

