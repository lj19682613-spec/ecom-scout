&lt;script&gt;
// ============================================================
// GSAP &amp; ScrollTrigger Setup
// ============================================================
gsap.registerPlugin(ScrollTrigger);

// Reveal animations
document.querySelectorAll('.reveal').forEach(el =&gt; {
  gsap.fromTo(el,
    { opacity: 0, y: 40 },
    {
      opacity: 1, y: 0, duration: 0.8, ease: 'power3.out',
      scrollTrigger: { trigger: el, start: 'top 88%', toggleActions: 'play none none none' }
    }
  );
});

// Progress bar
window.addEventListener('scroll', () =&gt; {
  const h = document.documentElement;
  const pct = (h.scrollTop / (h.scrollHeight - h.clientHeight)) * 100;
  document.getElementById('progress-bar').style.width = pct + '%';
});

// Hero entrance
gsap.timeline()
  .from('#hero-badge', { opacity: 0, y: -20, duration: 0.6, delay: 0.2 })
  .from('#hero-title', { opacity: 0, y: 30, duration: 0.8 }, '-=0.3')
  .from('#hero-sub', { opacity: 0, y: 20, duration: 0.6 }, '-=0.4')
  .from('#hero-date', { opacity: 0, duration: 0.5 }, '-=0.3')
  .from('#hero-counters &gt; *', { opacity: 0, y: 20, duration: 0.5, stagger: 0.1 }, '-=0.3');

// ============================================================
// Animated Counters
// ============================================================
function animateCounters() {
  document.querySelectorAll('.counter-value[data-target]').forEach(el =&gt; {
    const target = parseFloat(el.dataset.target);
    const suffix = el.dataset.suffix || '';
    const isFloat = target % 1 !== 0;
    let triggered = false;

    ScrollTrigger.create({
      trigger: el,
      start: 'top 90%',
      onEnter: () =&gt; {
        if (triggered) return;
        triggered = true;
        gsap.to({ val: 0 }, {
          val: target,
          duration: 1.8,
          ease: 'power2.out',
          onUpdate: function () {
            const v = this.targets()[0].val;
            el.textContent = (isFloat ? v.toFixed(1) : Math.round(v)) + suffix;
          }
        });
      }
    });
  });
}
animateCounters();

// Margin bar animations
document.querySelectorAll('.margin-bar-fill[data-width]').forEach(bar =&gt; {
  ScrollTrigger.create({
    trigger: bar,
    start: 'top 92%',
    onEnter: () =&gt; { bar.style.width = bar.dataset.width; }
  });
});

// ============================================================
// ECharts Shared Palette &amp; Config
// ============================================================
const PALETTE = {
  cyan: '#38bdf8', blue: '#3b82f6', violet: '#8b5cf6',
  emerald: '#34d399', amber: '#fbbf24', rose: '#f43f5e',
  orange: '#fb923c', slate: '#64748b', pink: '#ec4899',
  teal: '#2dd4bf'
};

function tooltipStyle() {
  return {
    backgroundColor: 'rgba(15, 23, 42, 0.95)',
    borderColor: 'rgba(56, 189, 248, 0.2)',
    borderWidth: 1,
    textStyle: { color: '#e2e8f0', fontFamily: "'Noto Sans SC','Inter',sans-serif", fontSize: 12 },
    extraCssText: 'border-radius: 12px; backdrop-filter: blur(8px); box-shadow: 0 8px 32px rgba(0,0,0,0.4);'
  };
}

function initChart(id) {
  const dom = document.getElementById(id);
  if (!dom) return null;
  const chart = echarts.init(dom, null, { renderer: 'svg' });
  window.addEventListener('resize', () =&gt; chart.resize());
  return chart;
}

// ============================================================
// Chart 1 — TikTok Revenue Top 10
// ============================================================
(function () {
  const chart = initChart('chart-tiktok-revenue');
  if (!chart) return;

  const data = [
    { name: 'BoomBoom\nNasal Stick', value: 18.7 },
    { name: 'Akunbem\nBikini Trimmer', value: 19.3 },
    { name: 'Merach\nVibration Plate', value: 20.5 },
    { name: 'NOBS\nToothpaste Tablets', value: 21.7 },
    { name: 'Medicube\nSkincare Set', value: 24.6 },
    { name: 'Wyze Cam\nSecurity Camera', value: 25.5 },
    { name: 'Homeika\nCordless Vacuum', value: 25.6 },
    { name: "MaryRuth's\nMultivitamin", value: 29.9 },
    { name: 'GOPURE\nNeck Cream', value: 32.3 },
    { name: 'Lip Liner\nStay-N-Peel', value: 46.6 }
  ];

  const colors = [
    PALETTE.slate, PALETTE.slate, PALETTE.teal, PALETTE.emerald,
    PALETTE.blue, PALETTE.blue, PALETTE.cyan, PALETTE.violet,
    PALETTE.rose, PALETTE.rose
  ];

  chart.setOption({
    tooltip: { ...tooltipStyle(), trigger: 'axis', axisPointer: { type: 'shadow' },
      formatter: p =&gt; `&lt;b&gt;${p[0].name.replace('\n',' ')}&lt;/b&gt;&lt;br/&gt;销售额: &lt;span style="color:${PALETTE.cyan};font-family:Space Mono"&gt;$${p[0].value}M&lt;/span&gt;` },
    grid: { left: 120, right: 40, top: 10, bottom: 20 },
    xAxis: { type: 'value', show: false },
    yAxis: {
      type: 'category',
      data: data.map(d =&gt; d.name),
      axisLine: { show: false }, axisTick: { show: false },
      axisLabel: { color: '#94a3b8', fontSize: 11, fontFamily: "'Inter','Noto Sans SC',sans-serif", lineHeight: 15 }
    },
    series: [{
      type: 'bar',
      data: data.map((d, i) =&gt; ({
        value: d.value,
        itemStyle: {
          color: new echarts.graphic.LinearGradient(0, 0, 1, 0, [
            { offset: 0, color: colors[i] + '33' },
            { offset: 1, color: colors[i] }
          ]),
          borderRadius: [0, 6, 6, 0]
        }
      })),
      barWidth: 22,
      label: {
        show: true, position: 'right',
        formatter: p =&gt; '$' + p.value + 'M',
        color: '#e2e8f0', fontSize: 12, fontFamily: 'Space Mono'
      },
      animationDuration: 1500,
      animationEasing: 'cubicOut'
    }]
  });

  ScrollTrigger.create({
    trigger: '#chart-tiktok-revenue',
    start: 'top 80%',
    onEnter: () =&gt; chart.resize()
  });
})();

