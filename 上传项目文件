        return `&lt;b&gt;${d[3]}&lt;/b&gt;&lt;br/&gt;预估利润率: &lt;span style="color:${PALETTE.emerald};font-family:Space Mono"&gt;${d[1]}%&lt;/span&gt;&lt;br/&gt;竞争度: ${compText}`;
      }
    },
    grid: { left: 80, right: 40, top: 40, bottom: 60 },
    xAxis: {
      name: '竞争度 →',
      nameTextStyle: { color: '#64748b', fontSize: 11, fontFamily: "'Noto Sans SC',sans-serif" },
      min: 0, max: 4,
      axisLine: { lineStyle: { color: '#1e293b' } },
      axisTick: { show: false },
      axisLabel: { show: false },
      splitLine: { lineStyle: { color: '#1e293b', type: 'dashed' } }
    },
    yAxis: {
      name: '利润率 % →',
      nameTextStyle: { color: '#64748b', fontSize: 11, fontFamily: "'Noto Sans SC',sans-serif" },
      min: 30, max: 100,
      axisLine: { lineStyle: { color: '#1e293b' } },
      axisTick: { show: false },
      axisLabel: { color: '#64748b', fontSize: 11, fontFamily: 'Space Mono', formatter: '{value}%' },
      splitLine: { lineStyle: { color: '#1e293b', type: 'dashed' } }
    },
    series: [{
      type: 'scatter',
      symbolSize: p =&gt; p[4],
      data: products.map(p =&gt; [p.comp, p.margin, p.size, p.name, p.size]),
      itemStyle: { shadowBlur: 16, shadowColor: 'rgba(56,189,248,0.2)' },
      label: {
        show: true, position: 'top',
        formatter: p =&gt; p.data[3],
        color: '#cbd5e1', fontSize: 11, fontFamily: "'Noto Sans SC',sans-serif"
      },
      animationDuration: 2000,
      color: products.map(p =&gt; p.color)
    }],
    // Quadrant area annotations
    graphic: [
      {
        type: 'text', left: 60, top: 30,
        style: { text: '⭐ 优选区域', fill: 'rgba(52,211,153,0.3)', fontSize: 14, fontFamily: "'Noto Sans SC',sans-serif" }
      }
    ]
  });

  // Color each point individually
  chart.setOption({
    series: [{
      data: products.map(p =&gt; ({
        value: [p.comp, p.margin, p.size, p.name, p.size],
        itemStyle: { color: p.color }
      }))
    }]
  });
})();

